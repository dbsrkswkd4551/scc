<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.scc.mapper.MemberMapper">




    <insert id="create" useGeneratedKeys="true" keyProperty="user_no">
    INSERT INTO member(user_id,user_password,user_name, user_birth,user_age,user_sex,user_phone,user_email)
           VALUES(#{user_id}, #{user_password}, #{user_name},#{user_birth}, #{user_age},#{user_sex}, #{user_phone},#{user_email})

    </insert>

    <insert id="createAuth">
		INSERT INTO member_auth
			 (
			   user_no,
			   auth
			 )
		VALUES
			 (
			   #{user_no},
			   #{auth}
			 )
	</insert>


</mapper>